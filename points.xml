<?xml version="1.0" encoding="UTF-8"?>
<!--
	pointControl
		server - Address of the master track daemon.
		port - Port number of the listener on the server.
		pointDaemon
			ident - Server identity for the daemon.
			count - The number of points controlled.
			point
				ident - Identity of the point.
				channel - The channel number on the controller.
				default - The default (straight ahead) position.
				turnout - The turnout position.
-->
<pointControl server="tinyseven.theknight.home" port="30331">
  <pointDaemon ident="1" count="11">
    <point ident="1" channel="0" default="290" turnout="320"/>
    <point ident="2" channel="1" default="290" turnout="320"/>
    <point ident="3" channel="2" default="290" turnout="320"/>
    <point ident="4" channel="3" default="290" turnout="320"/>
    <point ident="5" channel="4" default="290" turnout="320"/>
    <point ident="6" channel="5" default="290" turnout="320"/>
    <point ident="7" channel="6" default="290" turnout="320"/>
    <point ident="8" channel="7" default="290" turnout="320"/>
    <point ident="9" channel="8" default="290" turnout="320"/>
    <point ident="10" channel="9" default="290" turnout="320"/>
    <point ident="11" channel="10" default="290" turnout="320"/>
  </pointDaemon>
  <pointDaemon ident="2" count="11">
    <point ident="12" channel="0" default="290" turnout="335"/>
    <point ident="13" channel="1" default="290" turnout="320"/>
    <point ident="14" channel="2" default="305" turnout="265"/>
    <point ident="15" channel="3" default="290" turnout="320"/>
    <point ident="16" channel="4" default="305" turnout="275"/>
    <point ident="17" channel="5" default="275" turnout="232"/>
    <point ident="18" channel="6" default="290" turnout="320"/>
    <point ident="19" channel="7" default="290" turnout="320"/>
    <point ident="20" channel="8" default="290" turnout="320"/>
    <point ident="21" channel="9" default="290" turnout="320"/>
    <point ident="22" channel="10" default="290" turnout="320"/>
  </pointDaemon>
</pointControl>

